	email = matheus.felipe.19rt@gmail.com
	name = Matheus Mortari

[core]
	editor = code --wait
	autocrlf = false
	pager = less -+X
	quotepath = false
	preloadindex = true
	fscache = true

[init]
	defaultBranch = main

[push]
	default = current
	autoSetupRemote = true

[fetch]
	prune = true

[merge]
	conflictstyle = zdiff3

[diff]
	algorithm = histogram
	colorMoved = default
